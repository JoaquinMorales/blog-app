{
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "javascript.validate.enable": false,
        "import/no-named-as-default": 0,
        "import/prefer-default-export": 0
    },
    "extends": "airbnb",
    "env": {
        "jest": true
    },
    "parser": "babel-eslint",
    "plugins": ["flowtype"]
}